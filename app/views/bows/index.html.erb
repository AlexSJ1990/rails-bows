<div class="wrapper">
  <div class="cards">
    <% @bows.each do |bow|%>
      <div class="card">
        <%= link_to bow.description, bow_path(bow) %>
        <%= bow.price_cents %>
        <%= bow.size %>
        <p>Price: <%= humanized_money_with_symbol(bow.price) %></p>
      </div>
    <% end %>
  </div>
  <div class="nav">
    <%= link_to "Add a new bow", new_bow_path %>
  </div>
</div>


